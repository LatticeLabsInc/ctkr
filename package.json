{
  "name": "@ctkr/core",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "import": "./dist/stores/index.js"
    },
    "./stores/*": {
      "types": "./dist/stores/*.d.ts",
      "import": "./dist/stores/*.js"
    },
    "./constructs": {
      "types": "./dist/constructs/index.d.ts",
      "import": "./dist/constructs/index.js"
    },
    "./constructs/*": {
      "types": "./dist/constructs/*.d.ts",
      "import": "./dist/constructs/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -w -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}

